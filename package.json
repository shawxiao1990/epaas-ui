{
  "name": "epaas",
  "version": "1.0.0",
  "description": "paas platform",
  "author": "bingjunxiao",
  "scripts": {
    "webpack-serve": "WEBPACK_SERVE=true webpack-serve --config webpack.config.js",
    "webpack-dev-server": "NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "webpack-server": "NODE_ENV=production webpack-dev-server --config webpack.config.js",
    "webpack-cli": "webpack-cli",
    "dev": "vue-cli-service serve",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "preview": "node build/index.js --preview",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "lint": "eslint --ext .js,.vue src",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit"
  },
